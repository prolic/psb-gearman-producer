{
    "name": "prooph/psb-gearman-producer",
    "description": "Gearman Message Producer for Prooph Service Bus",
    "homepage": "https://github.com/bweston92/psb-gearman-producer",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "@todo"
        },
        {
            "name": "Alexander Miertsch",
            "email": "contact@prooph.de",
            "homepage": "http://www.prooph.de"
        }
    ],
    "keywords": [
        "prooph",
        "CQRS",
        "messaging",
        "gearman"
