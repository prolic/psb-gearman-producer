    "autoload-dev": {
        "psr-4": {
            "ProophTest\\ServiceBus\\": "vendor/prooph/service-bus/tests/",
            "ProophTest\\ServiceBus\\Message\\Gearman\\": "./tests"
        }
    },
    "suggest": {
        "container-interop/container-interop": "Allows use of Gearman producer factory."
    }
